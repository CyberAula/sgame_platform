<script>
	var settings = JSON.parse('<%=@game.settings.to_json.html_safe%>');
	$(document).ready(function(){
		$("#game").load(function(){
			try {
				document.getElementById("game").contentWindow.SGAME.loadSettings(settings);
			} catch (e){ }
		});
	});
</script>
<iframe id="game" class="gameTemplateIframe" src='<%=@game.template.url_full%>' width="100%" height="100%" iframeborder="0" frameborder="0" iframeElement.frameBorder = 0; webkitAllowFullScreen="true" allowfullscreen="true" mozallowfullscreen="true">
</iframe>